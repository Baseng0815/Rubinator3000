<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <!-- Put your settings here -->
  <appSettings>
    <add key="cameraFov" value="90" />
    <add key="cameraDistance" value="10" />
    <add key="viewerDelay" value="250" />
    <add key="mouseSensitivity" value="0.5" />
    <add key="scrollSensitivity" value="8" />
    <add key="moveAnimationTime" value="100" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="OpenTK" publicKeyToken="bad199fe84eb3df4" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>