<Window
    x:Class="Rubinator3000.CalibrationDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Rubinator3000"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="CalibrationDialog"
    Width="100"
    Height="80"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.5*" />
            <RowDefinition Height="0.5*" />
        </Grid.RowDefinitions>
        <ComboBox x:Name="ComboBox_CubeColorSelection" Grid.Row="0" />
        <Button
            x:Name="Button_FinishDialog"
            Grid.Row="1"
            Click="Button_FinishDialog_Click"
            Content="Finished" />
    </Grid>
</Window>
